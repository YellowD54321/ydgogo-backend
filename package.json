{
  "name": "delete-test-01",
  "description": "delete-test-01-description",
  "version": "0.0.1",
  "private": true,
  "dependencies": {
    "@aws-sdk/client-dynamodb": "^3.398.0",
    "@aws-sdk/lib-dynamodb": "^3.398.0",
    "google-auth-library": "^10.1.0",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.30.1",
    "@types/aws-lambda": "^8.10.0",
    "@types/jest": "^29.5.0",
    "@types/node": "^20.10.0",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^8.35.1",
    "@typescript-eslint/parser": "^8.35.1",
    "aws-sdk-client-mock": "^2.0.0",
    "dotenv-cli": "^8.0.0",
    "esbuild": "^0.19.0",
    "eslint": "^9.30.1",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import": "^2.32.0",
    "jest": "^29.2.1",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.0",
    "tsc-alias": "^1.8.16",
    "typescript": "^5.3.0"
  },
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
    "build": "tsc && tsc-alias",
    "clean": "rm -rf dist",
    "lint": "eslint src --ext .ts,.tsx",
    "lint:fix": "eslint src --ext .ts,.tsx --fix",
    "sam:build": "sam build",
    "sam:validate": "sam validate",
    "sam:local:api": "npx dotenv sam local start-api",
    "sam:local:test": "npx dotenv sam local invoke googleRegisterFunction --event events/event-googleRegister.json",
    "sam:deploy:dev": "sam build && sam deploy --config-env dev",
    "sam:deploy:prod": "sam build && sam deploy --config-env prod",
    "dev": "npm run sam:local:api",
    "deploy:dev": "npm run sam:deploy:dev",
    "deploy:prod": "npm run sam:deploy:prod"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "testMatch": [
      "**/__tests__/**/*.[jt]s?(x)",
      "**/?(*.)+(spec|test).[jt]s?(x)"
    ],
    "testPathIgnorePatterns": [
      "node_modules",
      "dist",
      "coverage",
      ".aws-sam"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "mjs"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "ts-jest"
    },
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/$1"
    }
  }
}
